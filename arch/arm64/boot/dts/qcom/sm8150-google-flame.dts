			regulator-initial-mode = <RPMH_REGULATOR_MODE_HPM>;
			regulator-allowed-modes = <RPMH_REGULATOR_MODE_LPM RPMH_REGULATOR_MODE_HPM>;
		};
		
		vreg_l15a_1p7: ldo15 { /* VRM */
			regulator-name = "pm8150_l15";
