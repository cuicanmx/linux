		vreg_s5a_2p0: smps5 { /* VRM */
			regulator-name = "pm8150_s5";
			regulator-min-microvolt = <2040000>;
			regulator-max-microvolt = <2040000>;
		};
		
		vreg_s6a_0p9: smps6 { /* VRM */
			regulator-name = "pm8150_s6";
